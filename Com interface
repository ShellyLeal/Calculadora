import java.io.*;

class CalculatorEngine { // classe correspondente às operações da calculadora
    char toDo;
    int value;
    int keep; // two calculator registers
    void binaryOperation(char op){
    keep = value; // manter o primeiro operando 
    value = 0; // inicializa o segundo operando
    toDo = op;
    }
    void add(){binaryOperation('+');}
    void subtract(){binaryOperation('-');}
    void multiply(){binaryOperation('*');}
    void divide(){binaryOperation('/');}
    void compute(){
    if(toDo=='+')
     value = keep+value;
    else if(toDo=='-')
     value = keep-value;
    else if(toDo=='*')
     value = keep*value;
    else if(toDo == '/')
     value = keep/value;
    keep= 0;
    }
    void clear(){
     keep = 0;
     value = 0;
    }
    void digit(int x){
     value = value*10 +x;    
    }
    int display(){
     return value;
    }
    CalculatorEngine(){clear();}
    
}

class CalculatorInput{ // classe para criar a interface simples
	BufferedReader stream;
	CalculatorEngine engine;

	
	CalculatorInput(CalculatorEngine e){
		InputStreamReader input = new InputStreamReader(System.in);
		stream = new BufferedReader(input);
		engine = e;
	}
	
	void run() throws Exception {
		for(;;) {
			System.out.print("["+engine.display()+"]"); // display personalizado
			String m = stream.readLine();
			if(m == null) break;
			if(m.length()>0){
				char c = m.charAt(0);
				if(c == '+') engine.add();
				else if(c=='-') engine.subtract();
				else if(c=='*') engine.multiply();
				else if(c=='/') engine.divide();
				else if(c>='0'&&c<='9') engine.digit(c-'0');
				else if(c=='=') engine.compute();
				else if(c=='c'||c=='C')engine.clear();		
				
			}
		}
	}
}

public class Calculadora {
	public static void main(String[] args) {
		CalculatorEngine c = new CalculatorEngine();
		CalculatorInput ci = new CalculatorInput(c);
		
		try{
			ci.run();
		} catch(Exception ex){}
		
        /*c.digit(1);
        c.digit(3);
        c.add();
        c.digit(1);
        c.digit(1);
        c.compute();*/
        System.out.println(c.display());
    }
}
